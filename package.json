{
	"name": "all-new-sugar-rush",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "powershell -Command \"Get-Process -Name 'node' -ErrorAction SilentlyContinue | Stop-Process -Force; vite dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "npm run build && wrangler deploy",
		"db:generate": "wrangler d1 execute all-new-sugar-rush --file=./schema.sql",
		"db:migrate": "wrangler d1 migrations apply all-new-sugar-rush"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/kit": "^2.37.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"wrangler": "^4.32.0"
	},
	"dependencies": {
		"lucide-svelte": "^0.541.0"
	}
}
